{
  "name": "one-dark-pro-italic",
  "displayName": "One Dark Pro Italic",
  "description": "Atom's OneDark Pro theme with italic comments and keywords!",
  "version": "3.1.2",
  "publisher": "markskelton",
  "bugs": {
    "url": "https://github.com/markypython/OneDark-Pro/issues"
  },
  "homepage": "https://github.com/markypython/OneDark-Pro",
