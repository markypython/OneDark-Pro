    "type": "git",
    "url": "https://github.com/markypython/OneDark-Pro"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "build": "node build.js",
    "pull-upstream": "git pull https://github.com/Binaryify/OneDark-Pro master"
  },
  "contributes": {
    "themes": [
      {
        "label": "One Dark Pro (Italic)",
        "uiTheme": "vs-dark",
